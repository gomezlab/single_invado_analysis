	Local-Url = {file://localhost/Users/mbergins/Documents/Papers/PDFs/Meyers,%20Current%20Biology--2006},
	Number = {17},
	Pages = {1685--1693},
	Title = {Potential for Control of Signaling Pathways via Cell Size and Shape},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/B6VRT-4KTNH9W-10/1/a6e498a3107df375dbc5b0837c301bc4},
	Volume = {16},
