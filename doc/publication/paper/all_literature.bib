	M3 = {10.1007/s00285-008-0179-x},
	Title = {Continuum model of cell adhesion and migration},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1007/s00285-008-0179-x},
	Year = {2008},
	Bdsk-File-1 = {},
