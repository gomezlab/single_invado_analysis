	Number = {19},
	Pages = {3475--3487},
	Title = {mDia2 regulates actin and focal adhesion dynamics and organization in the lamella for efficient epithelial cell migration},
	Ty = {JOUR},
	Url = {http://jcs.biologists.org/cgi/content/abstract/120/19/3475},
